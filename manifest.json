{
    "manifest_version": 3,
    "name": "Elementor Pro Admin",
    "version": "1.0",
    "description": "Elementor Pro админ-панели",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "cookies"
    ],
    "host_permissions": [
        "*://*.kwork.ru/*",
        "https://api.telegram.org/*"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "js/*",
                "css/*",
                "images/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}